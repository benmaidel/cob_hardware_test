<?xml version="1.0"?>
<launch>


	<!--include file="$(find cob_default_robot_config)/upload_param.launch" /-->    
	<include file="$(find cob_bringup_sim)/launch/robot.launch" />

    <include file="$(find cob_hardware_test)/components/calibration.test.xml" />
    <!-- start rviz with configuration -->
    <machine name="rviz_machine" address="$(env RVIZ_MACHINE)" ros-root="/opt/ros/electric/ros" ros-package-path="$(env ROS_PACKAGE_PATH)" user="fmw-ms" />
     
    <node machine="rviz_machine" pkg="rviz" type="rviz" name="$(anon rviz)" args="-d $(find cob_hardware_test)/config/calibration.vcg" respawn="false" output="screen" />


</launch>
