<?xml version="1.0"?>
<launch>

    <arg name="component"/>
    <arg name="robot" default="$(optenv ROBOT !!NO_ROBOT_SET!!)"/>

	<!-- perform test -->
    <test test-name="test_$(arg component)" pkg="cob_hardware_test_core" type="component_long_time_test.py" name="test_$(arg component)" time-limit="1800" >
        <rosparam command="load" file="$(find cob_bringup_test)/config/$(arg robot)/$(arg component).yaml" />
    </test>


</launch>
